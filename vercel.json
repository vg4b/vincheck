{
	"rewrites": [
		{
			"source": "/api/(.*)",
			"destination": "/api/$1"
		},
		{
			"source": "/((?!api/.*).*)",
			"destination": "/index.html"
		}
	],
	"headers": [
		{
			"source": "/api/(.*)",
			"headers": [
				{
					"key": "Access-Control-Allow-Methods",
					"value": "GET, OPTIONS"
				}
			]
		}
	]
}
